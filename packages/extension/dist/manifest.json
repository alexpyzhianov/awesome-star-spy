{
    "manifest_version": 2,
    "name": "Awesome Star Spy",
    "description": "Preview GitHub stars of every repository in an awesome list",
    "key": "",
    "version": "1.0.0",

    "browser_action": {
        "default_title": "Awesome Star Spy",
        "default_popup": "popup.html",
        "default_icon": "icon16.png"
    },

    "background": {
        "scripts": ["js/background.js"],
        "persistent": false
    },

    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },

    "permissions": [
        "identity",
        "activeTab",
        "tabs",
        "http://*/*",
        "https://*/*",
        "storage"
    ],

    "oauth2": {
        "client_id": "Iv1.097b63d40eafa56c",
        "scopes": [""]
    }
}
